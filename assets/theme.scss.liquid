.single-option-radio {
  border: 0;
  padding-top: 0;
  position: relative;
  background-color: $colorBody;

  input {
    -webkit-backface-visibility:hidden;-webkit-transform-style: preserve-3d;
    @include visuallyHidden;

    .lt-ie9 & {
      position: relative;
      height: auto;
      width: auto;
      clip: initial;
    }
  }

  label {
   font-family: 'Roboto', sans-serif ;
    position: relative;
    display: inline-block;
    line-height: 1.5;
    padding: 3px 5px 2px 5px;
    margin: 10px 4px 1px 3px;
    font-size: 15px; letter-spacing:1px;
    font-style: normal;
    background-color: $colorBody;
    border: 1px solid $colorBody;

    letter-spacing:1.3px;

    &.disabled:before {
      position: absolute;
      content: "";
      left: 50%;
      top: 3px;
      bottom: 0;
      border-left: 1px solid;
      border-color: #000;
      @include transform(rotate(90deg));

      .lt-ie9 & {
        display: none;
      }
    }

    &:active,
    &:focus {
      @media(min-width:1025px){
        background-color: adaptive-color($colorBody, 5%);
        border-color: adaptive-color($colorBody, 5%);
        font-weight:bold;
      }
    }
  }

  //style selected radio button's label
  input[type='radio']:checked + label {
    border-color: $colorTextBody;
  }

  //style label on input focus
  input[type='radio']:focus + label {
    background-color: adaptive-color($colorBody, 5%);
  }
}

.radio-wrapper .single-option-radio__label {
  display: block;
  margin-bottom: 10px;
  cursor: default;
  font-style: normal;
}
