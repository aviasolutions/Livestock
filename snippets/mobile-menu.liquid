


<nav class="cd-dropdown">
  <a href="#0" class="cd-close">Close</a>
  <ul class="cd-dropdown-content">
    {% assign menu = linklists[section.settings.main-menu] %}
    {% for link in menu.links %}
    {% if link.handle contains 'release-calendar' %}
    <li class="{% if megamenu %}has-children{% endif %}">
      <a href="{{ link.url }}">{{ link.title }}</a>
    </li>  
    {% endif %}
    {% endfor %}
    
    {% for link in menu.links %}
     {% assign megamenu = false %}
     {% assign link_handle = link.title | handleize %}
     {% assign new_block = '' %}
     {% for block in section.blocks %}
      {% assign block_menu_handle = block.settings.menu_item | handleize %}
    	{% if block_menu_handle == link_handle %}
    	  {% if block.settings.column1_enable or block.settings.column2_enable or block.settings.column3_enable %}
    		{% assign megamenu = true %}
    		{% assign new_block = block %}
    	  {% endif %}
    	{% endif %}
     {% endfor %}
    
    {% unless link.handle contains 'release-calendar' %}
     <li class="{% if megamenu %}has-children{% endif %}">
      <a href="{{ link.url }}">{{ link.title }}</a>
      {% if megamenu %}
        {% assign sub_menu_1 = '' %}
       {% if new_block.settings.column1_menu1 != blank %}
        {% assign sub_menu_1 = linklists[new_block.settings.column1_menu1] %}
       {% endif %}
       
       {% assign sub_menu_2 = '' %}
       {% if new_block.settings.column2_menu1 != blank %}
        {% assign sub_menu_2 = linklists[new_block.settings.column2_menu1] %}
       {% endif %}
       
       {% assign sub_menu_3 = '' %}
	   {% if new_block.settings.column3_menu1 != blank %}
        {% assign sub_menu_3 = linklists[new_block.settings.column3_menu1] %}
       {% endif %}
       
       {% assign sub_menu_4 = '' %}
       {% if new_block.settings.column4_menu1 != blank %}
        {% assign sub_menu_4 = linklists[new_block.settings.column4_menu1] %}
       {% endif %}
       <ul class="cd-secondary-dropdown is-hidden">
         <li class="go-back"><a href="#0">{{ link.title }}</a></li>
<!--          <li class="see-all"><a href="{{ link.url }}">{{ link.title }}</a></li> -->
         {% if sub_menu_1.links != blank %}
          
           {% if new_block.settings.menu-title-1 != blank %}
           <li class="sub_title sub_title_1">
             <a href="{% if new_block.settings.menu-url-1 != blank %}{{ new_block.settings.menu-url-1 }}{% else %}javascript:void(0){% endif %};">{{ new_block.settings.menu-title-1 }}</a>
           </li>
           {% endif %}
           {% for sub_menu in sub_menu_1.links %}
             <li {% if sub_menu.handle contains 'view-all' %}class="view_all__link"{% endif %}>
               <a href="{{ sub_menu.url }}">{{ sub_menu.title }}</a>
             </li>
           {% endfor %}
           
         {% endif %}
         
         {% if sub_menu_2.links != blank %}
          
           {% if new_block.settings.menu-title-3 != blank %}
           	<li class="sub_title sub_title_2">
             <a href="{% if new_block.settings.menu-url-3 != blank %}{{ new_block.settings.menu-url-3 }}{% else %}javascript:void(0){% endif %};">{{ new_block.settings.menu-title-3 }}</a>
           </li>
           {% endif %}
           {% for sub_menu in sub_menu_2.links %}
             <li {% if sub_menu.handle contains 'view-all' %}class="view_all__link"{% endif %}>
               <a href="{{ sub_menu.url }}">{{ sub_menu.title }}</a>
             </li>
           {% endfor %}
           
         {% endif %}
         
         {% if sub_menu_3.links != blank %}
          
           {% if new_block.settings.menu-title-5 != blank %}
             <li class="sub_title sub_title_3">
               <a href="{% if new_block.settings.menu-url-5 != blank %}{{ new_block.settings.menu-url-5 }}{% else %}javascript:void(0){% endif %};">{{ new_block.settings.menu-title-5 }}</a>
             </li>
           {% endif %}
           {% for sub_menu in sub_menu_3.links %}
             <li {% if sub_menu.handle contains 'view-all' %}class="view_all__link"{% endif %}>
               <a href="{{ sub_menu.url }}">{{ sub_menu.title }}</a>
             </li>
           {% endfor %}
           
         {% endif %}
         
          {% if sub_menu_4.links != blank %}
          
           {% if new_block.settings.menu-title-7 != blank %}
           	<li class="sub_title sub_title_7">
               <a href="{% if new_block.settings.menu-url-7 != blank %}{{ new_block.settings.menu-url-7 }}{% else %}javascript:void(0){% endif %};">{{ new_block.settings.menu-title-7 }}</a>
             </li>
           {% endif %}
           {% for sub_menu in sub_menu_4.links %}
             <li {% if sub_menu.handle contains 'view-all' %}class="view_all__link"{% endif %}>
               <a href="{{ sub_menu.url }}">{{ sub_menu.title }}</a>
             </li>
           {% endfor %}
           
         {% endif %}
         
         </ul> 
      {% endif %}
    </li> 
    {% endunless %}
	{% endfor %}
    

     {% for link in linklists[section.settings.top_menu].links %}
      <li class="name_{{link.title}} "> <a href="{{ link.url }}">{{ link.title }}</a> </li>
      {% endfor %}
        <li class="mobile-bottom-details">
      <div class="mobile-bottom-details-1">
