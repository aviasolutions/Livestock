                     data-widths="[180, 370, 590, 740, 900, 1080, 1296, 1512, 1728, 2048]"
                     data-aspectratio="{{ product.featured_image.aspect_ratio }}"
                     data-sizes="auto"
                     alt="{{ image.alt | escape }}" data-image-id="{{ image.id }}">
